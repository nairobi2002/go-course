package main

import (
	"fmt"
	"math/rand"
)

func main() {

	for i := 1; i <= 42; i++ {
		x := rand.Intn(5)
		fmt.Printf("\n-----Iteration %v: Value of x: %v-----\n", i, x)
		switch x {
		case 1:
			fmt.Printf("Number is 1")
		case 2:
			fmt.Printf("Number is 2")
		case 3:
			fmt.Printf("Number is 3")
		case 4:
			fmt.Printf("Number is 4")
		case 5:
			fmt.Printf("Number is 5")
		default:
			fmt.Printf("Number is 0")
		}
	}

}
