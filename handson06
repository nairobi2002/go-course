package main

import "fmt"

func main() {

	/*
		var a = "Hola Mundo"

		var b = 69

		var c = 3.14
	*/

	a, b, c := "Hola Mundo", 69, 3.14

	/*
		var a string = "Hola Mundo"

		var b int = 69

		var c float64 = 3.14
	*/

	fmt.Printf("Value of a: %v is of type %T\n", a, a)
	fmt.Printf("Value of b: %v is of type %T\n", b, b)
	fmt.Printf("Value of c: %v is of type %T\n", c, c)

}
